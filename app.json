  "flow": {
    "triggers": [
      {
        "id": "turned_on",
        "title": {
          "en": "TV is turned on"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=Samsung"
          }
        ]
      },
      {
        "id": "turned_off",
        "title": {
          "en": "TV is turned off"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=Samsung"
          }
        ]
      }
    ],
    "conditions": [
      {
        "id": "is_on",
        "title": {
          "en": "Is !{{on|off}}"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=Samsung"
          }
        ]
      },
      {
        "id": "is_app_running",
        "title": {
          "en": "Is app !{{running|not running}}"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=Samsung"
          },
          {
            "name": "app_id",
            "type": "autocomplete",
            "placeholder": {
              "en": "Search for app..."
            }
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "launch_app",
        "title": {
          "en": "Launch app"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=Samsung"
          },
          {
            "name": "app_id",
            "type": "autocomplete",
            "placeholder": {
              "en": "Search for app..."
            }
          }
        ]
      },
      {
